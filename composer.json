{
    "name": "eustasy/authenticatron",
    "description": "HOTP / TOTP secrets with corresponding QR links and code verification from a simple PHP script.",
    "homepage": "https://github.com/eustasy/authenticatron",
    "license": "MIT",
    "keywords": [
        "security",
        "authentication",
        "secret",
        "hotp",
        "totp",
        "passwords",
        "two-factor-authentication",
        "2fa webauthn"
    ],
    "autoload": {
        "classmap": [
            "_libs/",
            "authenticatron.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "11"
    },
    "scripts": {
        "test": [
            "phpunit",
            "@php authenticatron.php"
        ]
    }
}
